<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.dao.EmpDao">

    <cache type="com.baizhi.cache.RedisCache"/>

    <!--findAll-->
    <select id="findAll" resultType="Emp">
        select id,name,type,computer,mouse,other from t_emp
    </select>

    <!--save-->
    <insert id="save" parameterType="Emp" useGeneratedKeys="true" keyProperty="id">
        insert into t_emp values (#{id},#{name},#{type},#{computer},#{mouse},#{other})
    </insert>

    <!--delete-->
    <delete id="delete" parameterType="String">
        delete from t_emp where id =#{id}
    </delete>

    <!--findOne-->
    <select id="findOne" parameterType="String" resultType="Emp">
        select id,name,type,computer,mouse,other from t_emp where id = #{id}
    </select>

    <!--updateOne-->
    <update id="updateOne" parameterType="Emp">
        update t_emp set name = #{name},type =#{type},computer=#{computer},mouse=#{mouse},other=#{other} where id=#{id}
    </update>

    <select id="findPage" resultType="Emp">
         select id,name,type,computer,mouse,other from t_emp
    </select>

</mapper>